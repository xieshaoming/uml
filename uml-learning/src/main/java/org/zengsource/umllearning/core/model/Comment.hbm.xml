<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.zengsource.umllearning.core.model.Comment" table="tb_comment" lazy="false">
		<id name="id" column="c_id"></id>
		<property name="tid" column="t_id" type="string"></property>
		<property name="cname" column="c_name" type="string"></property>
		<property name="ccontent" column="c_content" type="string"></property>
		<property name="cposttime" column="c_posttime" type="timestamp"></property>
	</class>
</hibernate-mapping>